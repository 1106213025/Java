class Switch{
    public static void main(String[] args) {
        int num = 5;
        switch (num) {
            case 10:
            System.out.print("great");
            break;
            case 5:
            System.out.print("ok");
            break;
        default:
                  System.out.print("bad");
                  break;
        }

    }
    
}
